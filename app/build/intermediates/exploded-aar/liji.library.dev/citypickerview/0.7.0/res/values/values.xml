<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="colorAccent">#FF4081</color>
    <color name="colorPrimary">#3F51B5</color>
    <color name="colorPrimaryDark">#303F9F</color>
    <color name="liji_c_blue">#26A5FF</color>
    <color name="liji_material_blue_500">#03a9f4</color>
    <color name="liji_material_blue_700">#0288d1</color>
    <color name="liji_material_red_500">#e51c23</color>
    <color name="liji_material_red_700">#d01716</color>
    <color name="province_line_border">#C7C7C7</color>
    <declare-styleable name="CityPickerView"><attr format="color" name="title_background"/><attr format="color" name="text_confirm_color"/><attr format="color" name="text_cancel_color"/><attr format="color" name="wheel_color"/><attr format="color" name="wheel_text_color"/><attr format="dimension" name="wheel_text_size"/><attr format="dimension" name="title_text_size"/><attr format="dimension" name="title_action_size"/></declare-styleable>
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <string name="app_name">CityPicker</string>
    <style name="AnimBottom" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/push_bottom_out</item>
    </style>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
</resources>